*&---------------------------------------------------------------------*
*&      Module  READ_SPFLI  OUTPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE read_spfli OUTPUT.

  select single * from spfli
    into spfli
    where carrid = sflight-carrid
      and connid = sflight-connid.

ENDMODULE.
*&---------------------------------------------------------------------*
*&      Module  READ_SCARR  OUTPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE read_scarr OUTPUT.
  select single * from scarr
    into scarr
    where carrid = sflight-carrid.
ENDMODULE.

